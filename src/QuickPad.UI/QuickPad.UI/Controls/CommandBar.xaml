                </AppBarButton.Icon>
                <ToolTipService.ToolTip>
                    <TextBlock x:Uid="CompactOverlayCommandTooltip" Text="Compact Overlay (Alt + Up)" />
                </ToolTipService.ToolTip>
            </AppBarButton>
        </CommandBar>
    </Grid>
</UserControl>
